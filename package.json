{
  "name": "xp.js-styled",
  "version": "1.1.0",
  "description": "Build performant styled components for Web, iOS and Android.",
  "license": "MIT",
  "main": "index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "compilerOptions": {
    "types": [
      "react",
      "jest"
    ]
  },
  "keywords": [
    "modern",
    "react",
    "style-library",
    "react-hooks",
    "frontend",
    "library",
    "css",
    "styleable",
    "styled"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "format": "yarn prettier . --write",
    "reset": "git reset --hard origin/main",
    "release": "bash ./scripts/release.sh"
  },
  "prettier": {
    "printWidth": 140,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lestercorderomurillo/xp.js-styled.git"
  },
  "author": "Lester Cordero Murillo",
  "bugs": {
    "url": "https://github.com/lestercorderomurillo/xp.js-styled/issues"
  },
  "homepage": "https://github.com/lestercorderomurillo/xp.js-styled#readme",
  "dependencies": {
    "@types/react": "^18.3.3",
    "react": "^18.3.1",
    "react-native": "^0.74.1",
    "type-fest": "^4.18.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
